@article{Therriault:2006p2805,
author = {David J Therriault and Mike Rinck and Rolf A Zwaan}, 
journal = {Memory {\&} cognition},
title = {Assessing the influence of dimensional focus during situation model construction},
abstract = {According to Zwaan, Langston, and Graesser's (1995) event-indexing model, when comprehending text, readers monitor changes in a series of critical dimensions: space, time, protagonist, causality, and intentionality. In this study, the influence of dimensional focus was assessed during situation-model construction. Participants read narratives and were instructed to specifically monitor a single dimension while their sentence reading times were recorded. Critical sentence reading times were then analyzed for all shift types. Results support the general prediction that at least the time and protagonist dimensions are resistant to task demands, demonstrating that comprehenders routinely perform dimensional updating processes that are context independent. These results are discussed in the context of the event-indexing model.},
