@article{Shafto:2003p886,
author = {Patrick Shafto and John D Coley}, 
journal = {Journal of experimental psychology. Learning, memory, and cognition},
title = {Development of categorization and reasoning in the natural world: novices to experts, naive similarity to ecological knowledge.},
abstract = {Two experiments investigate the role of similarity and causal-ecological knowledge in expert and novice categorization and reasoning. In Experiment 1, university undergraduates and commercial fishermen sorted marine creatures into groups; although there was substantial agreement, novices sorted largely on the basis of appearance, whereas experts often cited commercial, ecological, or behavioral factors, and systematically subdivided fish on the basis of ecological niche. In Experiment 2, experts and novices were asked to generalize a blank property or novel disease from a pair of marine creatures. Novices relied on similarity to guide generalizations. Experts used similarity to reason about blank properties but ecological relations to reason about diseases. Expertise appears to involve knowledge of multiple relations among entities and context-sensitive application of those relations.},
affiliation = {Department of Psychology, Northeastern University, Boston, Massachusetts 02115-5000, USA. p.shafto@neu.edu},
