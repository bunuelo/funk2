@article{Purves:2001p171,
author = {D Purves and R B Lotto and S M Williams and S Nundy and Z Yang}, 
journal = {Philos. Trans. R. Soc. Lond., B, Biol. Sci.},
title = {Why we see things the way we do: evidence for a wholly empirical strategy of vision.},
abstract = {Many otherwise puzzling aspects of the way we see brightness, colour, orientation and motion can be understood in wholly empirical terms. The evidence reviewed here leads to the conclusion that visual percepts are based on patterns of reflex neural activity shaped entirely by the past success (or failure) of visually guided behaviour in response to the same or a similar retinal stimulus. As a result, the images we see accord with what the sources of the stimuli have typically turned out to be, rather than with the physical properties of the relevant objects. If vision does indeed depend upon this operational strategy to generate optimally useful perceptions of inevitably ambiguous stimuli, then the underlying neurobiological processes will eventually need to be understood within this conceptual framework.},
affiliation = {Duke University Medical Center, Durham, NC 27710, USA. purves@neuro.duke.edu},
