@article{Berg:2003p27661,
author = {Johannes Berg and Michael L{\"a}ssig}, 
journal = {arXiv},
title = {Local graph alignment and motif search in biological networks},
abstract = {Interaction networks are of central importance in post-genomic molecular biology, with increasing amounts of data becoming available by high-throughput methods. Examples are gene regulatory networks or protein interaction maps. The main challenge in the analysis of these data is to read off biological functions from the topology of the network. Topological motifs, i.e., patterns occurring repeatedly at different positions in the network have recently been identified as basic modules of molecular information processing. In this paper, we discuss motifs derived from families of mutually similar but not necessarily identical patterns. We establish a statistical model for the occurrence of such motifs, from which we derive a scoring function for their statistical significance. Based on this scoring function, we develop a search algorithm for topological motifs called graph alignment, a procedure with some analogies to sequence alignment. The algorithm is applied to the gene regulation network of E. coli.},
eprint = {cond-mat/0308251v2},
volume = {cond-mat.stat-mech},
year = {2003},
month = {Jan},
keywords = {q-bio.MN, cond-mat.stat-mech}, 
