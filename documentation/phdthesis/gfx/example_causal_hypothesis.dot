digraph G {
  
  subgraph cluster_1 {
    label = "{\\huge{The $\\text{Reflective}^1$ Layer}}";
    color=blue
    
    // reflective
    node [fillcolor = white, color = "#0000CC", style = filled];
    edge [color = "#0000CC"];
        
    // t_1^*
    
    t_1 [texlbl = "{\huge{\tt{t-1*}}}"];
    
    // t_2^*
    
    t_2 [texlbl = "{\huge{\tt{t-2*}}}"];
    
    
    // reflective_1
    
    reflective_1 [texlbl = "{\huge{$\text{\tt{reflective}}^{1*}$}}"];
    
    reflective_1_past_t_1 [shape=box,style=filled,label="{\huge{\tt{past*}}}"];
    reflective_1 -> reflective_1_past_t_1 [arrowhead=none];
    reflective_1_past_t_1 -> t_1 [style="-triangle 45"];

    reflective_1_future_t_2 [shape=box,style=filled,label="{\huge{\tt{future*}}}"];
    reflective_1 -> reflective_1_future_t_2 [arrowhead=none];
    reflective_1_future_t_2 -> t_2 [style="-triangle 45"];
    
    // trans_1^*
    
    trans_1 [texlbl = "{\huge{\tt{trans-1*}}}"];
    
    trans_1_reflecting [shape=trapezium,style=filled,label="{\huge{$\Psi$}}",nodesep="0.02",margin="0.11,0.055";
    trans_1 -> trans_1_reflecting [style=dashed,arrowhead=none];
    trans_1_reflecting -> reflective_1 [style="-triangle 45,dashed"];
    trans_1_reflecting -> reflective_1_past_t_1 [style="-triangle 45,dashed"];
    trans_1_reflecting -> t_1 [style="-triangle 45,dashed"];
    trans_1_reflecting -> reflective_1_future_t_2 [style="-triangle 45,dashed"];
    trans_1_reflecting -> t_2 [style="-triangle 45,dashed"];
    
  }
}
