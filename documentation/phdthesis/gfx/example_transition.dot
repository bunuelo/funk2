    x3_block_2_sitting_on_x3_block_1 -> x3_block_1 [style="-triangle 45",color=red];
    
    x_3 [texlbl = "{\huge{\tt{Symbol*}}}"];
    x_3_reflecting [shape=trapezium,style=filled,label="{\huge{$\Psi$}}",nodesep="0.02",margin="0.11,0.055";
