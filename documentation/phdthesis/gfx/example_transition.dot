  subgraph cluster_1 {
    label = "{\\huge{The $\\text{Reflective}^1$ Layer}}";
    color=blue
    
    // reflective
    node [fillcolor = white, color = "#0000CC", style = filled];
    edge [color = "#0000CC"];
        
    // x_2^*
    
    x2_block_2   [texlbl = "{\huge{\tt{Block}}}",color=red];
    x2_table_1   [texlbl = "{\huge{\tt{Table}}}",color=red];
    
