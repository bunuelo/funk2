[defunk em_two-documentation-all_packages []
  [let [[top_packages     `[moral_compass
			    mindmon]]
	[package_name_set [new set]]]
    [mapc [funk [package_name]
		[mapc [funk [node]
			    [have package_name_set add [get node label]]]
		      [get [get [get [lookup-package package_name] dependency_graph] node_set] elements]]]
	  top_packages]
    [get package_name_set elements]]]

