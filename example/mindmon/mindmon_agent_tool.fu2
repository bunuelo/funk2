[deframe mindmon_agent_tool [frame] [mindmon_agent_widget]
  [new [initial-mindmon_agent_widget]
       [= mindmon_agent_widget initial-mindmon_agent_widget]
       
       ]]



'children of mindmon_agent_tool must implement the interface below.'

[deftypefunk mindmon_agent_tool get mindmon_agent_tool-title []
  [terminal_format standard-terminal '\nmindmon_agent_tool-get-title not implemented in ' [type this] ' interface.']
  nil]

[deftypefunk mindmon_agent_tool execute mindmon_agent_tool-start []
  [terminal_format standard-terminal '\nmindmon_agent_tool-get-start not implemented in ' [type this] ' interface.']
  nil]

