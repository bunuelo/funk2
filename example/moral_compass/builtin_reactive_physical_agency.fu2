[deframe turn_right_start_resource        [resource] [] [new [] [construct resource `turn_right_start        [funk []               [have [get this body] turn_right-start]]]]]
[deframe turn_right_stop_resource         [resource] [] [new [] [construct resource `turn_right_stop         [funk []               [have [get this body] turn_right-stop]]]]]
