[deframe builtin_reactive_physical_agency [agency] []
  [new []
       [construct agency `physical]
       
       'isis_world agent specific physical commands'
       [have this add_resource [new turn_right_start_resource]]
       [have this add_resource [new turn_right_stop_resource]]
       [have this add_resource [new turn_left_start_resource]]
       [have this add_resource [new turn_left_stop_resource]]
       [have this add_resource [new look_up_start_resource]]
       [have this add_resource [new look_up_stop_resource]]
       [have this add_resource [new look_down_start_resource]]
       [have this add_resource [new look_down_stop_resource]]
       [have this add_resource [new move_forward_start_resource]]
       [have this add_resource [new move_forward_stop_resource]]
       [have this add_resource [new move_backward_start_resource]]
       [have this add_resource [new move_backward_stop_resource]]
       [have this add_resource [new drop_from_right_hand_resource]]
       [have this add_resource [new drop_from_left_hand_resource]]
       [have this add_resource [new pick_up_with_right_hand_resource]]
       [have this add_resource [new pick_up_with_left_hand_resource]]
       [have this add_resource [new use_right_hand_resource]]
       [have this add_resource [new use_left_hand_resource]]
       [have this add_resource [new say_resource]]
       
       this]]


