'leveling'
[deframe level_pitch_resource [traced_resource] []
  [new []
       [construct traced_resource `level_pitch
		  [funk []
			[have this-resource level_pitch]]]
       nil]]

[deftypefunk level_pitch_resource execute level_pitch[]
  [let* [[body  [get this body]]
	 [mind  [get body mind]]
	 [name  [get body name]]]
    [let[[level_neck_pitch [- 0 15]]
	 [slightly_looking_up 5]
	 [slightly_looking_down [- 0 24]]]
      
      [have this-resource think_to_self 'Leveling pitch is starting.']
      [while [ > [- [get_neck_pitch mind] level_neck_pitch] slightly_looking_up]
	[have this-resource think_to_self 'looking down, pitch = '[get_neck_pitch mind]]
	[mind-wait_to_activate mind learned_reactive physical look_down 1 [* [ abs [get_neck_pitch mind]] 33]]]
      [while [ < [- [get_neck_pitch mind] level_neck_pitch] slightly_looking_down]
	[have this-resource think_to_self 'looking up, pitch = ' [get_neck_pitch mind]]
	[mind-wait_to_activate mind learned_reactive physical look_up 1  [* [ abs [get_neck_pitch mind]] 33] ]]
      [have this-resource think_to_self 'leveling pitch finished']]]]


