[deframe stand_in_front_of_resource [traced_resource] []
  [new []
       [construct traced_resource `stand_in_front_of
		  [funk [object]
			[have this-resource stand_in_front_of object]]]
       nil]]

 

[deftypefunk stand_in_front_of_resource execute stand_in_front_of [object]
  [let* [[body  [get this body]]
	 [mind  [get body mind]]
	 [name  [get body name]]]
    [have this-resource think_to_self 'stand_in_front_of' object 'activated!']
    `[mind-wait_to_activate mind learned_reactive physical align_body_with_neck]
    `[while [> [abs [get_neck_roll mind]] 8]
       [sleep 4]]
    `[mind-wait_to_activate mind learned_reactive physical center_body_on object]   
    `[while [or [null [get_visual_objects mind]] [null [have [get_visual_objects mind] look_up object]]]
       [sleep 5]]
    `[while [> [abs [have [have [get_visual_objects mind] lookup object] lookup `retina_position_horizontal]] 0.4]
       [sleep 5]]
    [labels [[get_orientation           [object]
					[have [have [get mind perception] lookup object] lookup `orientation]]
	     [get_distance              [object]
					[have [have [get mind perception] lookup object] lookup `distance]]
	     [find_final_distance       [orientation_diff init_distance]
					[let [[c 
					       [get [- [+ [* init_distance init_distance] 
							  25]
						       [* [* [get abs orientation_diff degree_cosine] init_distance ] 10]] power 0.5]]
					      ]
					  c]]
	     [find_init_angle           [orientation_diff init_distance]
					[let* [[c 
						[sqrt [- [+ [* init_distance init_distance] 
							    25]
							 [* [* [degree_cosine orientation_diff] init_distance ] 10]]]]
					       [cos_beta
						[/ [- [+ [* init_distance init_distance]
							 [* c c]]
						      25]
						   [* init_distance c]]]
					       [beta [degree_arccosine cos_beta]]
					       ]
					  beta]]
	     
	     [find_turning_speed        [init_angle]
					[* [init_angle]] 33]
	     [should_i_turn_right       [init_orientation]
					[if [< init_orientation 0]
					    t
					  nil]]
	     [should_i_move_forward     [current_neck_angle final_angle]
					[if [< current_neck_angle final_angle]
					    t
					  nil]
					]]
    [let* [[init_orientation            [get_orientation object]]
	   [init_orientation_difference [ - [abs 180] [abs init_orientation]]]
	   [init_distance               [get_distance object]]
	   [init_angle                  [find_init_angle init_orientation_difference init_distance]]
	   [init_speed                  [* 33 [init_angle]]]
	   [final_neck_angle            [+ [ init_orientation_difference init_angle]]]
	   [final_distance              [find_final_distance init_orientation_difference init_distance]]
	   [init_turning_speed          [find_turning_speed init_angle]]
	   [turn_right                  [should_i_turn_right init_orientation]]
	   ]
      [format stout 'init_orientation' init_orientation 'init_diff' init_orientation_difference 'init_distance'  init_distance 'init_angle'  init_angle 'final_neck_angle'  final_neck_angle]
       [if [= init_orientation 180]
	  [if [< init_distance 5]
	      [mind-wait_to_activate mind learned_reactive physical move_backward 1 [* 33 [- 5 init_distance]]]
	    [mind-wait_to_activate mind learned_reactive physical move_forward 1 [* 33 [- init_distance 5]]]]
	[if turn_right
	    [prog [mind-wait_to_activate mind learned_reactive physical turn_right 1 init_speed]
		  [mind-wait_to_activate mind learned_reactive physical look_left 1 init_speed]]
	  [prog [mind-wait_to_activate mind learned_reactive physical turn_left 1 init_speed]
		[mind-wait_to_activate mind learned_reactive physical look_right 1 init_speed]]]]
      
	[while [> [abs [-[get_neck_roll mind] final_angle]] 8]
	  [if [should_i_move_forward [get_orientation object] init_orientation]
	      [prog [mind-wait_to_activate mind learned_reactive physical move_forward 1 20.0]
		    [center_head_on object]]
	  [prog [mind-wait_to_activate mind learned_reactive physical move_backward 1 20.0]
		[center_head_on object]]]
	  [mind-wait_to_activate mind learned_reactive physical center_head_on object]
	  [mind-wait_to_activate mind learned_reactive physical align_body_with_neck]]      
	
	]]]]

