	  [table [get mind look_for-current_object]]]
			    
      [mind-wait_to_activate mind learned_reactive physical use_right_hand table `put_on ]
      [mind-wait_to_activate mind learned_reactive physical use_left_hand loaf `divide]
      [mind-wait_to_activate mind learned_reactive physical look_for `toaster]
      [let 
	  [[toaster   [have [get [get [get mind layer `learned_reactive] agency `physical] resource `look_for] lookup `my_current_object]]]
	
	[mind-wait_to_activate mind learned_reactive physical use_right_hand toaster `put_in ]
	[mind-wait_to_activate mind learned_reactive physical use_right_hand toaster `turn_on]
	[mind-wait_to_activate mind learned_reactive physical use_right_hand toaster `turn_off]]
      [mind-wait_to_activate mind learned_reactive physical hold_with_right_hand loaf ]
      ]]]


