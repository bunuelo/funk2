[deframe align_body_with_neck_resource [traced_resource] []
  [new []
       [construct traced_resource `align_body_with_neck
		  [funk []
			[have this-resource align_body_with_neck]]]
       nil]]

[deftypefunk align_body_with_neck_resource execute align_body_with_neck []
  [let* [[body  [get this body]]
	 [mind  [get body mind]]
	 [name  [get body name]]]
    [let[[init_diff [get_neck_roll mind]]
	 [slightly_looking_left 8]
	 [slightly_looking_right [- 0 8]]]
      [labels [[adjust_neck [patience]
			    [print 'adjusting neck']
			    [if [> patience 0]
				[cond [[ > [get_neck_roll mind] slightly_looking_left]
				       [prog [print 'my head is facing the left']
					     [mind-wait_to_activate mind learned_reactive physical look_right 1 [* 10 [abs [get_neck_roll mind]]]]
					     [adjust_neck [- patience 1]]]]
				      [[ < [- [get_neck_pitch mind] 0] slightly_looking_right]
				       [prog [print 'my head is facing the right']
					     [mind-wait_to_activate mind learned_reactive physical look_left 1 [* 10 [abs [get_neck_roll mind]]]]
					     [adjust_neck [- patience 1]]]]
				      [t
				       [have this-resource think_to_self 'I am looking forward']]]
			      [have this-resource think_to_self 'I give up']]]]
	       
	      [adjust_neck 15]
	      [cond [[ > [- init_diff 0] slightly_looking_left]
		     [mind-wait_to_activate mind learned_reactive physical turn_left 1 [* 33 init_diff]]]
		    [[ < [- init_diff 0] slightly_looking_right]
		     [mind-wait_to_activate mind learned_reactive physical turn_right 1 [* 33 init_diff]]]
		    [t
		     [have this-resource think_to_self 'no need to turn']]]
	      [have this-resource think_to_self 'finished']]]]]
	      

