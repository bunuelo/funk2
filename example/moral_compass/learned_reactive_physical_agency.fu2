[deftypefunk look_at_floor_resource execute look_at_floor[]
  [let* [[body  [get this body]]
	 [mind  [get body mind]]
	 [name  [get body name]]]
    [let[[floor_pitch [- 0 50]]
	 [off_set          10]]
      [if [< [get_neck_pitch mind] floor_pitch]
	  [have this-resource think_to_self 'already looking at floor']
	[prog 
	 [have this-resource think_to_self 'looking at floor is starting.']
	 [while [and [> [get_neck_pitch mind] floor_pitch]
		     [> [- [get_neck_pitch mind] floor_pitch] off_set]]
	   [have this-resource think_to_self 'looking down, pitch = '[get_neck_pitch mind]]
	   [mind-wait_to_activate mind learned_reactive physical look_down 1 [* [abs [- [get_neck_pitch mind] floor_pitch ]] 33]]]]]
      [have this-resource think_to_self 'looking at floor finished']]]]


