[deframe self_conscious_imprimer_agency [agency] []
  [new []
       [construct agency `imprimer]
       
       [have this add_resource [new resource `react_to_imprimer_scold
				    [funk [imprimer scold]
					  [have this think_to_self 'Uh oh, my imprimer, ' [get imprimer name] ', has scolded something about ' [get scold focus] '!  How should I react?']
					  ]]]
       
       this]]

