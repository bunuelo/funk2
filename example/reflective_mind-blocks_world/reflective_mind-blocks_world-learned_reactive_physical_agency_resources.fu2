  [new []
       [construct learned_reactive_resource `drop_block_and_wait_for_block_to_fall
		  [funk []
			
			[have [get this mind] wait_to_call_physical_action `[drop]]

			[dotimes [i 8]
			  [wait-for-trigger [get [get [get this mind] body] time_step_trigger]]]
			
			]]]]

[deftypefunk reflective_mind-blocks_world-drop_block_and_wait_for_block_to_fall_resource get as-english_conslist []
  `[drop block and wait for block to fall]]


