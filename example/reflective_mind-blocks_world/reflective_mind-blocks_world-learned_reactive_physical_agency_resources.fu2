'reflective_mind-blocks_world-attempt_to_grab_block_until_gripper_stops_moving'

[deframe reflective_mind-blocks_world-attempt_to_grab_block_until_gripper_stops_moving_resource [learned_reactive_resource] []
  [new []
       [construct learned_reactive_resource `attempt_to_grab_block_until_gripper_stops_moving
		  [funk []
			
			[have [get this mind] wait_to_call_physical_action `[grab]]
			
			[dotimes [i 10]
			  [wait-for-trigger [get [get [get this mind] body] time_step_trigger]]]
			
			[have [get this mind] wait_for_physical_property `gripper `is-me `t `movement_command `stop]
			
			]]]]

[deftypefunk reflective_mind-blocks_world-attempt_to_grab_block_until_gripper_stops_moving_resource get as-english_conslist []
  `[attempt to grab block until gripper stops moving]]


'reflective_mind-blocks_world-drop_block_and_wait_for_block_to_fall'

