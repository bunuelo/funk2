'reflective_mind-blocks_world-move_left_until_over_pyramid'

[deframe reflective_mind-blocks_world-move_left_until_over_pyramid_resource [learned_reactive_resource] []
  [new []
       [construct learned_reactive_resource `move_left_until_over_pyramid
		  [funk []
			[have       [get this mind] wait_to_call_physical_action `[move left]]
			[have-apply [get this mind] wait_for_physical_relationship `[block shape pyramid preposition below gripper is-me t]]
			[have       [get this mind] wait_to_call_physical_action `[stop]]
			]]]]

[deftypefunk reflective_mind-blocks_world-move_left_until_over_pyramid_resource get as-english_conslist []
  `[move left until over pyramid]]


