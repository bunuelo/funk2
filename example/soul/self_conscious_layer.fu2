'self_conscious_layer'

[deframe          self_conscious_layer [frame] [mental_layer
						imprimer_agency]]
[deftypeconstruct self_conscious_layer []
  [set this mental_layer [new mental_layer 4 `self_conscious]]
  [set this imprimer_agency [new self_conscious_imprimer_agency]]
  [have [get this mental_layer] add_agency [get this imprimer_agency]]
  this]

