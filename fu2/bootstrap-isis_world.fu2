[deframe isis_world_client [frame] [xmlrpc_url]]

[deftypeconstruct isis_world_client [xmlrpc_url]
  [set this xmlrpc_url xmlrpc_url]
  this]

[deftypefunk isis_world_client execute sense                   []                   [xmlrpc-funkall [get this xmlrpc_url] `do `sense]]
[deftypefunk isis_world_client execute turn_left-start         []                   [xmlrpc-funkall [get this xmlrpc_url] `do `turn_left-start]]
[deftypefunk isis_world_client execute turn_left-stop          []                   [xmlrpc-funkall [get this xmlrpc_url] `do `turn_left-stop]]
[deftypefunk isis_world_client execute turn_right-start        []                   [xmlrpc-funkall [get this xmlrpc_url] `do `turn_right-start]]
[deftypefunk isis_world_client execute turn_right-stop         []                   [xmlrpc-funkall [get this xmlrpc_url] `do `turn_right-stop]]
[deftypefunk isis_world_client execute move_forward-start      []                   [xmlrpc-funkall [get this xmlrpc_url] `do `move_forward-start]]
[deftypefunk isis_world_client execute move_forward-stop       []                   [xmlrpc-funkall [get this xmlrpc_url] `do `move_forward-stop]]
[deftypefunk isis_world_client execute move_backward-start     []                   [xmlrpc-funkall [get this xmlrpc_url] `do `move_backward-start]]
[deftypefunk isis_world_client execute move_backward-stop      []                   [xmlrpc-funkall [get this xmlrpc_url] `do `move_backward-stop]]
