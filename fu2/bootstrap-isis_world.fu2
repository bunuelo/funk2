[deftypefunk isis_world_client execute look_up-stop            []                   [xmlrpc-funkall [get this xmlrpc_url] `do `look_up-stop]]
[deftypefunk isis_world_client execute look_down-start         []                   [xmlrpc-funkall [get this xmlrpc_url] `do `look_down-start]]
