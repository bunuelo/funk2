[defunk concept_version_space-test_hypothesis_orderings []
  [let [[h1  [new concept_version_space_hypothesis t]]
	[h1a [new concept_version_space_hypothesis t]]
	[h2  [new concept_version_space_hypothesis nil]]]
    [if [not [get h1 is_consistent_with_hypothesis    h1a]] [error bug_type `concept_version_space-test_hypothesis_orderings number 1]]
    [if [not [get h1 is_more_general_than_hypothesis  h2]]  [error bug_type `concept_version_space-test_hypothesis_orderings number 2]]
    [if      [get h2 is_more_general_than_hypothesis  h1]   [error bug_type `concept_version_space-test_hypothesis_orderings number 3]]
    [if [not [get h2 is_more_specific_than_hypothesis h1]]  [error bug_type `concept_version_space-test_hypothesis_orderings number 4]]
    [if      [get h1 is_more_specific_than_hypothesis h2]   [error bug_type `concept_version_space-test_hypothesis_orderings number 5]]
    
    nil]]

[defunk concept_version_space-test []
  [concept_version_space-test_hypothesis_orderings]
  [let [[cvs [new concept_version_space]]
	[e1  [let [[this [new concept_version_space_example t]]]
	       [have this add_variable_value `x 1]
	       [have this add_variable_value `y 1]
	       this]]
	]
    [have cvs train_on_example e1]
    cvs]]

