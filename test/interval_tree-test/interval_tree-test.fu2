[defunk interval_tree-test []
  [let [[interval_tree [new interval_tree
			    [funk [x] [have x lookup `left]]
			    [funk [x] [have x lookup `right]]
			    [funk [x y] [== x y]]
			    [funk [x y] [< x y]]
			    [funk [x y] [/ [+ x y] 2.0]]
			    ]]]
    [let [[element_count 100]]
      [let [[element_array [new array element_count]]]
	
	[dotimes [i element_count]
	  [let [[element [frame left [random 1000] right [random 1000]]]]
	    [have interval_tree insert element]
	    [set element_array elt i element]]]]]
    
    interval_tree]]

